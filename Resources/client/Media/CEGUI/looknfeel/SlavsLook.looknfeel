                        </OperatorDim>
                    </Dim>
                    <Dim type="BottomEdge" >
                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="1" type="BottomEdge" />
                            <ImageDim name="SlavsLook/TooltipBottomEdge" dimension="Height" />
                        </OperatorDim>
                    </Dim>
