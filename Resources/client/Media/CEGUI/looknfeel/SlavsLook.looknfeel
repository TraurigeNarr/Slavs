                <HorzFormat type="RightAligned" />
            </ImageryComponent>
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge" ><ImageDim name="SlavsLook/NewTitlebarLeft" dimension="Width" /></Dim>
                    <Dim type="RightEdge" >
                        <OperatorDim op="Subtract">
                            <OperatorDim op="Subtract">
                                <OperatorDim op="Subtract">
                                    <UnifiedDim scale="1" type="RightEdge"/>
                                    <ImageDim name="SlavsLook/SysAreaRight" dimension="Width"/>
                                </OperatorDim>
                                <ImageDim name="SlavsLook/SysAreaMiddle" dimension="Width"/>
                            </OperatorDim>
                            <ImageDim name="SlavsLook/NewTitlebarRight" dimension="Width" />
                        </OperatorDim>
                    </Dim>
                </Area>
                <Image name="SlavsLook/NewTitlebarMiddle" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
