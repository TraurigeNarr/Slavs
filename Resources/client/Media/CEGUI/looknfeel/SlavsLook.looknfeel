                <HorzFormat type="RightAligned" />
            </ImageryComponent>
            <ImageryComponent>
                <Area>
                    <Dim type="RightEdge" >
                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="1" type="RightEdge"/>
                            <ImageDim name="SlavsLook/SysAreaRight" dimension="Width" />
                        </OperatorDim>
                    </Dim>
                </Area>
                <Image name="SlavsLook/SysAreaMiddle" />
                <VertFormat type="Stretched" />
                <HorzFormat type="RightAligned" />
            </ImageryComponent>
            <ImageryComponent>
                <Image name="SlavsLook/SysAreaRight" />
