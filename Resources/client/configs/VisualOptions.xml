<VisualOptions>
	<!--Image sets that will be loaded-->
	<ImageSets>
		<ImageSet path="SlavsGameCommands.imageset"/>
    <ImageSet path="Lesovik.imageset"/>
	</ImageSets>
	<Schemes>
		<Scheme path="SlavsGameCommands.scheme"/>
	</Schemes>
	<Buttons>
		<Undefined>
			<ButtonType>SlavsLook/Button</ButtonType>
			<Tooltip>Define image for this button</Tooltip>	
			<DisplayName>No name button</DisplayName>
			<DisplayText>true</DisplayText>
		</Undefined>
		<Create.Production.Store>
			<ButtonType>SlavsGameCommands/CreateProductionStore</ButtonType>
			<Tooltip>Create production store to keep resources in it</Tooltip>	
			<DisplayName>Production Store</DisplayName>
			<DisplayText>false</DisplayText>
		</Create.Production.Store>
		<Create.Manufacture>
			<ButtonType>SlavsGameCommands/CreateManufacture</ButtonType>
			<Tooltip>Manfacture hires humans and gives them money for production of resources.</Tooltip>	
			<DisplayName>Manufacture</DisplayName>
			<DisplayText>false</DisplayText>
		</Create.Manufacture>
		<Create.Hizhina>
			<ButtonType>SlavsGameCommands/CreateHizhina</ButtonType>
			<Tooltip>Humans live in house.</Tooltip>	
			<DisplayName>Hizhina</DisplayName>
			<DisplayText>false</DisplayText>
		</Create.Hizhina>
		<Production>
			<ButtonType>SlavsLook/Button</ButtonType>
			<Tooltip>Production of resources</Tooltip>	
			<DisplayName>Production</DisplayName>
			<DisplayText>true</DisplayText>
		</Production>
		<Society>
			<ButtonType>SlavsLook/Button</ButtonType>
			<Tooltip>Production of resources</Tooltip>	
			<DisplayName>Society</DisplayName>
			<DisplayText>true</DisplayText>
		</Society>
	</Buttons>
  <Information>
    <Lesovik.Information type="information">
      <Image>Lesovik/Lesovik_Information</Image>
      <Caption>Lesovik</Caption>
      <Text>Lesovik do not likes when someone destroys tree</Text>
      <Id>0</Id>
    </Lesovik.Information>
    <FirstHistory type="clip">
      <Caption>History</Caption>
      <Id>1</Id>
      <Clip>
        <Frame show_time="1.5">
          <Image>Lesovik/Lesovik_Information</Image>
          <Text>Once upon a time when Gods decide to create a world</Text>
        </Frame>
        <Frame show_time="1.0">
          <Image>Lesovik/Lesovik_Information</Image>
          <Text>And they create us</Text>
        </Frame>
      </Clip>
    </FirstHistory>
    <VelesHistory type="clip">
      <Caption>History</Caption>
      <Id>2</Id>
      <Clip>
        <Frame show_time="1.5">
          <Image>Lesovik/Lesovik_Information</Image>
          <Text>Once upon a time when Gods decide to create a world</Text>
        </Frame>
        <Frame show_time="1.0">
          <Image>Lesovik/Lesovik_Information</Image>
          <Text>And they create us</Text>
        </Frame>
      </Clip>
    </VelesHistory>
  </Information>
</VisualOptions>